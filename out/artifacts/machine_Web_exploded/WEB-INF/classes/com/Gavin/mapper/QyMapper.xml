<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.Gavin.mapper.QyMapper">
    <select id="selectcount" resultType="int">
        select count(*) from machinedata
    </select>
    
    <select id="selectByPage" resultType="com.Gavin.bean.Machinedata">
        select * from machinedata limit #{param1},#{param2}
    </select>
</mapper>
